{
  "name": "eslint-config-typescript-shareable",
  "version": "1.6.0",
  "description": "Common TypeScript lint configuration (eslint, stylelint, prettier)",
  "main": "index.js",
  "files": [
    "index.js",
    ".stylelintrc",
    ".prettierrc.js"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/AxisCommunications/eslint-config-typescript-shareable.git"
  },
  "homepage": "https://github.com/AxisCommunications/eslint-config-typescript-shareable",
  "license": "MIT",
  "engines": {
    "node": ">=10.11.0",
    "yarn": ">=1.12.3"
  },
  "scripts": {
    "lint": "yarn prettier:check",
    "prettier:check": "prettier -c -u .",
    "prettier:fix": "prettier -c -u -w .",
    "release": "sbin/release.sh"
  },
  "peerDependencies": {
    "@typescript-eslint/eslint-plugin": "^5.0.0",
    "@typescript-eslint/parser": "^5.0.0",
    "eslint": "^8.0.0",
    "eslint-plugin-functional": "^4.0.0",
    "eslint-plugin-import": "^2.23.4",
    "eslint-plugin-react": "^7.24.0",
    "eslint-plugin-react-hooks": "^4.2.0",
    "prettier": "^2.3.0",
    "stylelint": "^13.13.1",
    "stylelint-config-recommended": "^5.0.0",
    "stylelint-config-styled-components": "^0.1.1",
    "stylelint-processor-styled-components": "^1.10.0",
    "typescript": "^4.2.4",
    "typescript-styled-plugin": "^0.17.0"
  },
  "devDependencies": {
    "prettier": "2.5.1",
    "semver": "7.3.5"
  },
  "packageManager": "yarn@3.1.1"
